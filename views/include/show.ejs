<% for(var i=0;i<users.length;i++){ %>
<tr>
    <td><%= i+1 %></td>
    <td><%= users[i].name %></td>
    <td><%= users[i].email %></td>
    <td><%= users[i].gender %></td>
    <td><%= users[i].status %></td>
    <td class="">
        <a href="/update-user?id=<%=users[i]._id%>" class="btn border-shadow updatebtn">
            <span class="text-gradient text-info"><i class="fa-solid fa-pen-to-square"></i></span>
        </a>
        <a href="#" class="btn border-shadow deletebtn" data-id=<%=users[i]._id%> >
            <span class="text-gradient text-danger"><i class="fa-solid fa-trash"></i></span>
        </a>
    </td>
</tr>
<% } %>